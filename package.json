{
  "name": "hnswlib-wasm",
  "version": "0.0.5-beta.2",
  "description": "typescript and wasm bindings for Hnswlib",
  "main": "./dist/hnswlib-wasm.umd.cjs",
  "module": "./dist/hnswlib-wasm.js",
  "types": "./dist/hnswlib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hnswlib-wasm.js",
      "require": "./dist/hnswlib-wasm.umd.cjs"
    }
  },
  "type": "module",
  "repository": "https://github.com/shravansunder/hnswlib-wasm",
  "homepage": "https://github.com/shravansunder/hnswlib-wasm",
  "author": "Shravan Sunder<shravan.sunder.dev@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "hnswlib",
    "approximate nearest neighbor search",
    "machine learning",
    "wasm",
    "typescript",
    "emscripten",
    "embeddings"
  ],
  "scripts": {
    "prepare": "husky install",
    "build:emcc": "make",
    "build": "yarn vite build",
    "build:all": "yarn build:emcc && yarn build",
    "test": "jest",
    "publish:pre": "npm version prerelease --git-tag-version false && npm publish --tag next"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/emscripten": "^1.39.6",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@xn-sakina/phoenix": "^1.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "fake-indexeddb": "^4.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^3.2.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsembind": "^1.1.0",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-commonjs": "^0.6.2",
    "vite-plugin-dts": "^2.2.0",
    "vite-plugin-static-copy": "^0.13.1",
    "vite-plugin-top-level-await": "^1.3.0",
    "vite-plugin-wasm": "^3.2.2",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.29.8"
  },
  "packageManager": "yarn@3.5.0",
  "files": [
    "dist",
    "lib",
    "Makefile",
    "package.json",
    "README.md",
    "LICENSE"
  ]
}
